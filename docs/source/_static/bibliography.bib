@article{chung_computation-ready_2014,
	title = {Computation-{Ready}, {Experimental} {Metal}{\textendash}{Organic} {Frameworks}: {A} {Tool} {To} {Enable} {High}-{Throughput} {Screening} of {Nanoporous} {Crystals}},
	volume = {26},
	issn = {0897-4756, 1520-5002},
	shorttitle = {Computation-{Ready}, {Experimental} {Metal}{\textendash}{Organic} {Frameworks}},
	url = {http://pubs.acs.org/doi/10.1021/cm502594j},
	doi = {10.1021/cm502594j},
	abstract = {Experimentally refined crystal structures for metal- organic frameworks (MOFs) often include solvent molecules and partially occupied or disordered atoms. This creates a major impediment to applying high-throughput computational screening to MOFs. To address this problem, we have constructed a database of MOF structures that are derived from experimental data but are immediately suitable for molecular simulations. The computationready, experimental (CoRE) MOF database contains over 4700 porous structures with publically available atomic coordinates. Important physical and chemical properties including the surface area and pore dimensions are reported for these structures. To demonstrate the utility of the database, we performed grand canonical Monte Carlo simulations of methane adsorption on all structures in the CoRE MOF database. We investigated the structural properties of the CoRE MOFs that govern methane storage capacity and found that these relationships agree well with those derived recently from a large database of hypothetical MOFs.},
	language = {en},
	number = {21},
	urldate = {2019-04-07},
	journal = {Chemistry of Materials},
	author = {Chung, Yongchul G. and Camp, Jeffrey and Haranczyk, Maciej and Sikora, Benjamin J. and Bury, Wojciech and Krungleviciute, Vaiva and Yildirim, Taner and Farha, Omar K. and Sholl, David S. and Snurr, Randall Q.},
	month = nov,
	year = {2014},
	pages = {6185--6192},
	file = {Chung et al. - 2014 - Computation-Ready, Experimental Metal{\textendash}Organic Fram.pdf:/home/kevin/Zotero/storage/WNVL6XTN/Chung et al. - 2014 - Computation-Ready, Experimental Metal{\textendash}Organic Fram.pdf:application/pdf}
