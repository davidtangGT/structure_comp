	pages = {2618--2625},
	file = {ACS Full Text PDF w/ Links:/home/kevin/Zotero/storage/WQW3XGDN/Moghadam et al. - 2017 - Development of a Cambridge Structural Database Sub.pdf:application/pdf;ACS Full Text Snapshot:/home/kevin/Zotero/storage/G6W7FBM8/acs.chemmater.html:text/html}
}

@article{sturluson_role_2019,
