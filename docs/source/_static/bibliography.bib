	doi = {10.1021/acs.chemmater.7b00441},
	abstract = {We report the generation and characterization of the most complete collection of metal{\textendash}organic frameworks (MOFs) maintained and updated, for the first time, by the Cambridge Crystallographic Data Centre (CCDC). To set up this subset, we asked the question {\textquotedblleft}what is a MOF?{\textquotedblright} and implemented a number of {\textquotedblleft}look-for-MOF{\textquotedblright} criteria embedded within a bespoke Cambridge Structural Database (CSD) Python API workflow to identify and extract information on 69 666 MOF materials. The CSD MOF subset is updated regularly with subsequent MOF additions to the CSD, bringing a unique record for all researchers working in the area of porous materials around the world, whether to perform high-throughput computational screening for materials discovery or to have a global view over the existing structures in a single resource. Using this resource, we then developed and used an array of computational tools to remove residual solvent molecules from the framework pores of all the MOFs identified and went on to analyze geometrical and physical properties of nondisordered structures.},
	number = {7},
	urldate = {2019-04-07},
	journal = {Chemistry of Materials},
	author = {Moghadam, Peyman Z. and Li, Aurelia and Wiggin, Seth B. and Tao, Andi and Maloney, Andrew G. P. and Wood, Peter A. and Ward, Suzanna C. and Fairen-Jimenez, David},
	month = apr,
